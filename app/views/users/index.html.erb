<div class="container">
 <div class="row">

 <%= render 'users/list_user', { user: @user, book_new: @book_new } %>

	<div class="col-xs-9">
		<h2>Users</h2>
		<table class="table">
			<tr>
				<td>image</td>
				<td>name</td>
				<td></td>
			</tr>
			<% @users.each do |user| %>
			<tr>
				<td> <%= attachment_image_tag user, :profile_image, size:"50x50",
					format:"jpeg", fallback:"no_image.jpg" %> </td>
				<td> <%= user.name %> </td>
				<td> <%= link_to "show", user_path(user) %> </td>
			</tr>
			<% end %>
		</table>
	</div>

 </div>
</div>